<h1>Admin::FootPrints#index</h1>
<p>Find me in app/views/admin/foot_prints/index.html.erb</p>

<div class="container">
    <table class="table">
      <thead>
        <tr>
          <th scope="col">Title</th>
          <th scope="col">result</th>
          <th scope="col">User</th>
          <th scope="col">Show</th>
          <th scope="col">Delete</th>
          
        

        </tr>
      </thead>
      <tbody>
    
<% @foot_print.each do |foot| %>
        <tr>
          <td> <%= foot.product.title %> </td>
          <td><%= foot.result %></td>
          <td><%= link_to foot.user.first_name , admin_user_path(foot.user.id)%></td>
          <td><%= link_to "more", admin_foot_print_path(foot.id) %>
          <td><%= link_to "Delete", admin_foot_print_path(foot.id), data: {:confirm => "Are you sure?"}, :method => :delete%></td>
        </tr>
<% end %>
        <tr>
      </tbody>
    </table>
<div id="menu">
<%= link_to "Modifier le profil", edit_user_registration_path(current_user.id), class:"nav-link"%>
</div>

